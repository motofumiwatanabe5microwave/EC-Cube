{% extends 'default_frame.twig' %}

{% set mypageno = 'resetting' %}

{% set body_class = 'mypage' %}

{% block main %}
    <div class="ec-layoutRole__main">
        <div class="ec-mypageRole">
            <div class="ec-pageHeader">
                <h1>{{ 'front.mypage.title'|trans }}/{{ 'front.mypage.nav__resetting_complete'|trans }}</h1>
            </div>
        </div>
        <div class="ec-registerCompleteRole">
            <div class="ec-reportHeading">
                <h2>{{ 'front.mypage.resetting_complete_message__title'|trans }}</h2>
            </div>
            <div class="ec-off2Grid ec-text-ac">
                <div class="ec-off2Grid__cell">
                    <p class="ec-reportDescription">{{ 'front.mypage.resetting_complete_message__body'|trans }}</p>
                </div>
            </div>
            <div class="ec-off4Grid">
                <div class="ec-off4Grid__cell">
                    <a class="ec-blockBtn--cancel" href="{{ url('homepage') }}">{{ 'common.back'|trans }}</a>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
